# Environment Variables Template
# Copy this file to .env.local and update the values

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://yourdomain.com

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Booking Configuration
NEXT_PUBLIC_CALCOM_USERNAME=your_calcom_username
NEXT_PUBLIC_CALCOM_EVENT_TYPE=your_event_type_name
NEXT_PUBLIC_CALCOM_API_KEY=your_calcom_api_key
NEXT_PUBLIC_CALENDLY_URL=https://calendly.com/your-username/intro

# Analytics
NEXT_PUBLIC_PLAUSIBLE_DOMAIN=yourdomain.com

# A/B Testing (optional)
EXPERIMENT_VARIANT=A

# Additional (optional)
# RESEND_API_KEY=your_resend_api_key
# SMTP_HOST=your_smtp_host
# SMTP_PORT=587
# SMTP_USER=your_smtp_user
# SMTP_PASS=your_smtp_password
